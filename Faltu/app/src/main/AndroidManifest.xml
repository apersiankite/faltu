<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zoomcar.prateek.faltu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".activities.SampleLoginActivity" />
        <activity
            android:name=".activities.NavingationActivity"
            android:label="@string/app_name">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.ComponentActivity"
            android:label="@string/title_activity_component"></activity>
        <activity
            android:name=".activities.CustomProgressBarActivity"
            android:label="@string/title_activity_custom_progress_bar" />
        <activity
            android:name=".activities.TestingActionBarActivity"
            android:label="@string/label_activity_testing_actionbar"
            android:theme="@style/AppTheme.ActionBarTheme"></activity>
        <activity
            android:name=".activities.HorizontalSliderActivity"
            android:label="@string/title_activity_horizontal_slider">


        </activity>
        <activity
            android:name=".activities.ToggleSwitchActivity"
            android:label="@string/title_activity_toggle_switch">

        </activity>
        <activity
            android:name=".activities.colorPalleteActivity"
            android:label="@string/title_activity_color_pallete"></activity>

        <activity android:name=".activities.CalendarActivity"></activity>

        <activity android:name=".activities.FBLoginActivity">

        </activity>

        <activity android:name=".activities.TestFBLoginActivity">

        </activity>

        <activity android:name=".activities.SampleCardActivity">

        </activity>

        <activity android:name=".activities.ZoomMainActivity">


        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

    </application>

</manifest>
